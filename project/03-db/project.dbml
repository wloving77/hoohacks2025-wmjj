Table users {
    userid integer [primary key]
    username varchar
    issues list
}

Table issues {
    issue_id integer [primary key]
    issue varchar
}

Table executive {
    article_id integer [primary key]
    article varchar
    date datetime
    summary varchar
    issue_id integer
    priority integer
    fed_state_loc varchar
}

Table legislative {
    article_id integer [primary key]
    article varchar
    date datetime
    summary varchar
    issue_id integer
    priority integer
    fed_state_loc varchar
}

Table judicial {
    article_id integer [primary key]
    article varchar
    date datetime
    summary varchar
    issue_id integer    
    priority integer
    fed_state_loc varchar
}